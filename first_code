namespace ConsoleApp16
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Console.Write("enter your number");
                int number = Console.ReadLine();
            if (number % 2 == 0)
            {
                Console.Write("zog");
            }
            else
            {
                Console.Write("fard");
            }
        }
    }
}
